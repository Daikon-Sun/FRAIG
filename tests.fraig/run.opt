#! /bin/csh
if ($#argv == 0) then
  echo "Missing aag file. Using -- run.opt xx for (optxx.aag)"; exit 1
endif

set design=opt$1.aag
if (! -e $design) then
   echo "$design does not exists" ; exit 1
endif

set dofile=do.opt
rm -f $dofile
echo "cirr $design" > $dofile
echo "cirp" >> $dofile
echo "cirp -pi" >> $dofile
echo "cirp -po" >> $dofile
echo "cirp -n" >> $dofile
echo "cirp -fl" >> $dofile
echo "cirsw" >> $dofile
echo "ciropt" >> $dofile
echo "cirstrash" >> $dofile
echo "cirp" >> $dofile
echo "cirp -pi" >> $dofile
echo "cirp -po" >> $dofile
echo "cirp -n" >> $dofile
echo "cirp -fl" >> $dofile
echo "q -f" >> $dofile
../fraig -f $dofile
